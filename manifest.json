{
  "manifest_version": 2,

  "name": "jwplayer downloader",
  "description": "This extension downloads new streamings of jwplayer hosting service.",
  "version": "0.1",

  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  }
}